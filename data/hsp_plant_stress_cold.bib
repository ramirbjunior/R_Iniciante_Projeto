@article{ ISI:000224622100012,
Author = {Zhang, X and Guo, SL and Yin, HB and Xiong, DJ and Zhang, H and Zhao, YX},
Title = {{Molecular cloning and identification of a heat shock cognate protein 70
   gene, Thhsc70, in Thellungiella halophila}},
Journal = {{ACTA BOTANICA SINICA}},
Year = {{2004}},
Volume = {{46}},
Number = {{10}},
Pages = {{1212-1219}},
Month = {{OCT}},
Abstract = {{Heat shock cognate proteins 70 (hsp70s) act as molecular chaperones.
   Some hsp70s are also expressed in unstressed plants, known as hsc70. To
   gain further knowledge about the hsc70, the Thellungiella halophila
   hsc70 (Thhsc70) gene that encoded the cytosolic hsc70 in salt cress ( T
   halophila (C. A. Mey.) O. E. Schulz) was identified. In unstressed
   plants the expression of Thhsc70 was shown to be tissue-specific. The
   Thhsc70 gene was induced by heat and cold stresses, but almost not by
   salt and drought stresses. Overexpression of Thhsc70 could increase
   thermotolerance and chilling tolerance in transgenic Arabidopsis plants.}},
