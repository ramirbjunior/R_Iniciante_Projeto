@article{ ISI:000397823400002,
Author = {Jabeen, Bushra and Naqvi, S. M. Saqlan and Mahmood, Tariq and Sultana,
   Tasawar and Arif, Madiha and Khan, Fariha},
Title = {{Ectopic Expression of Plant RNA Chaperone Offering Multiple Stress
   Tolerance in E-coli}},
