@article{ ISI:000233698200015,
Author = {Zhou, SP and Sauve, RJ and Abdulah, A},
Title = {{Identification of genes regulated by low temperature in Pachysandra
   terminalis Sieb. et Zucc using cDNA differential display}},
Journal = {{HORTSCIENCE}},
Year = {{2005}},
Volume = {{40}},
Number = {{7}},
Pages = {{1995-1997}},
Month = {{DEC}},
Abstract = {{Complementary Deoxyribonucleic Acid (cDNA) differential display and
   reverse Northern dot blot were used to identify genes in Pachysandra
   terminalis Sieb. \& Zucc., a cold-tolerant plant, that are regulated by
   low temperatures. Rooted cuttings were obtained from stock plants that
   had been maintained in a greenhouse at 24 degrees C. These cuttings were
   subjected to the following cold treatments: 2 weeks at 12 degrees C, 48
   hours at 4 degrees C, 48 hours at 0 degrees C, and 4 hours at -1 degrees
   C. Following leaf tissue analysis of treated and control plants, some
   stress-related genes and many novel genes were identified. Northern blot
   hybridization demonstrated that all novel genes were regulated by the
   cold treatments.}},
