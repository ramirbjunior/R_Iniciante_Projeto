Author = {Zinn, Kelly E. and Tunc-Ozdemir, Meral and Harper, Jeffrey F.},
Title = {{Temperature stress and plant sexual reproduction: uncovering the weakest
   links}},
Journal = {{JOURNAL OF EXPERIMENTAL BOTANY}},
Year = {{2010}},
Volume = {{61}},
