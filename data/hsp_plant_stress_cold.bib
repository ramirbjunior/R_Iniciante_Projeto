@article{ ISI:A1994NH73100003,
Author = {SINGLA, SL and GROVER, A},
Title = {{DETECTION AND QUANTITATION OF A RAPIDLY ACCUMULATING AND PREDOMINANT 104
   KDA HEAT-SHOCK POLYPEPTIDE IN RICE}},
Journal = {{PLANT SCIENCE}},
Year = {{1994}},
Volume = {{97}},
Number = {{1}},
Pages = {{23-30}},
Abstract = {{When 5-day-old rice seedlings were subjected to a high temperature
   stress (45 degrees C), the shoots as well as roots synthesized and
   accumulated a 104 kDa polypeptide within 1-2 h of the imposition of the
   stress. The higher levels of this polypeptide persisted for nearly 16 h
   when the 45 degrees C temperature stress was continued for 24 h or up to
   4 h when seedlings were brought to recovery at 28 degrees C after 4 h of
   45 degrees C temperature stress. The leaves obtained from mature plants
   also accumulated the same polypeptide in response to temperature stress.
   Exposure of seedlings to salinity, desiccation, cold, wounding or
   exogenous abscisic acid application had no effect on the accumulation of
   this polypeptide. Importantly, this polypeptide made nearly 0.4\% of the
   total soluble protein fraction. High temperature induced accumulation of
   this polypeptide was noted to be conserved in three indica and three
   japonica cultivars of Oryza sativa tested in this study.}},
