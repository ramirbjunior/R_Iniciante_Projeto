@article{ ISI:000077994300004,
Author = {Angelova, A and Chakarov, S and Marincheva, B},
Title = {{Immunochemical approach for analysis of stress response in wheat
   seedlings affected by different external factors}},
Journal = {{BIOTECHNOLOGY \& BIOTECHNOLOGICAL EQUIPMENT}},
Year = {{1998}},
Volume = {{12}},
Number = {{2}},
Pages = {{23-26}},
Abstract = {{An immunochemical approach for comparative analysis of protein changes
   in wheat seedlings subjected to different stress factors was developed.
   It is based on the production of antibodies against a total protein
   extract from roots of cold stressed wheat seedlings with the presumption
   for a high prevailance of the stress proteins in the early stages of the
   cold stress response due to the reduced rate of the overall
   physiological processes. The polyclonal antiserum was affinity purified
   and enriched by filtering against a total protein extract from control
   seedlings. The western blot analysis revealed protein profiles specific
   for the applied stress factors. Thus, the method allowes simultaneous
   analysis of different stress factors. It is quick and sensitive and can
   be suggested as a preliminary test in selection of stress tolerant
   agriculture plants and as an alternative to the use of radiolabeled
   compounds.}},
