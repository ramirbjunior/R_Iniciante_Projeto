@article{ ISI:000317804300003,
Author = {Swigonska, Sylwia and Weidner, Stanislaw},
Title = {{Proteomic analysis of response to long-term continuous stress in roots
   of germinating soybean seeds}},
Journal = {{JOURNAL OF PLANT PHYSIOLOGY}},
