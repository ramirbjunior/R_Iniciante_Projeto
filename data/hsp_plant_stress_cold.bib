@article{ ISI:A1995RD29800013,
Author = {MORRELL, AM and WAMPLE, RL},
Title = {{THERMOTOLERANCE OF DORMANT AND ACTIVELY GROWING CABERNET-SAUVIGNON IS
   IMPROVED BY HEAT-SHOCK}},
Journal = {{AMERICAN JOURNAL OF ENOLOGY AND VITICULTURE}},
Year = {{1995}},
Volume = {{46}},
Number = {{2}},
Pages = {{243-249}},
Abstract = {{Actively growing Cabernet Sauvignon plants grown at 25 degrees C were
   given a four-hour heat shock at 40 degrees C, followed by a four-hour
   incubation period at 25 degrees C. The heat shocked plants, when
   compared to non-heat shocked plants, had less leaf damage, tissue water
   loss, and electrolyte leakage after exposure to 50 degrees C. Dormant
   Cabernet Sauvignon cuttings were removed from 3 degrees C cold storage
   and heat shocked for 30 minutes at either 45 degrees C or room
   temperature (23 degrees C). This was followed by a four-hour incubation
   period at room temperature. A group of non-heat shocked control cuttings
   were held continuously at 3 degrees C. Thermotolerance was tested by
   placing the cuttings in water baths at 54 degrees C, 56 degrees C, 58
   degrees C, or 60 degrees C for 30 minutes. Primary, secondary and
   tertiary bud survival, time required for buds to begin growth, and root
   vigor were evaluated for each treatment. Bud survival was high in all
   treatments at 54 degrees C and 56 degrees C, while at 60 degrees C there
   was 100\% bud mortality in all treatments. At 58 degrees C, however,
   cuttings that were heat shocked at room temperature were significantly
   (p < 0.01) more thermotolerant than the control cuttings, and those that
   were heat shocked at 45 degrees C were significantly (p = 0.01) more
   heat tolerant than those heat shocked at room temperature. These results
   show that both dormant and actively growing grape tissues respond to
   heat shock treatment by increasing their thermotolerance. To our
   knowledge, this is the first time that heat shock responses have been
   shown to occur in dormant woody plant tissue. Heat shock treatment could
   lead to more effective pathogen elimination by allowing grape
   propagation material to be treated at higher temperatures than are
   currently possible.}},
