@article{ ISI:000372256900068,
Author = {Fan, Meihua and Sun, Xue and Xu, Nianjun and Liao, Zhi and Wang, Rixin},
Title = {{cDNA cloning, characterization and expression analysis of manganese
   superoxide dismutase in Ulva prolifera}},
Journal = {{JOURNAL OF APPLIED PHYCOLOGY}},
