@article{ ISI:000272375900009,
Author = {Yi, Shu-ying and Liu, Jian},
Title = {{Combinatorial Interactions of Two cis-Acting Elements, AT-Rich Regions
   and HSEs, in the Expression of Tomato Lehsp23.8 upon Heat and Non-Heat
   Stresses}},
