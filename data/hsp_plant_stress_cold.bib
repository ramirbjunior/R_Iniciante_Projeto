@article{ ISI:000276618400002,
Author = {Janska, A. and Marsik, P. and Zelenkova, S. and Ovesna, J.},
Title = {{Cold stress and acclimation - what is important for metabolic
   adjustment?}},
Journal = {{PLANT BIOLOGY}},
Year = {{2010}},
Volume = {{12}},
Number = {{3}},
Pages = {{395-405}},
Month = {{MAY}},
Abstract = {{As sessile organisms, plants are unable to escape from the many abiotic
   and biotic factors that cause a departure from optimal conditions of
   growth and development. Low temperature represents one of the most
   harmful abiotic stresses affecting temperate plants. These species have
   adapted to seasonal variations in temperature by adjusting their
   metabolism during autumn, increasing their content of a range of
   cryo-protective compounds to maximise their cold tolerance. Some of
   these molecules are synthesised de novo. The down-regulation of some
   gene products represents an additional important regulatory mechanism.
   Ways in which plants cope with cold stress are described, and the
   current state of the art with respect to both the model plant
   Arabidopsis thaliana and crop plants in the area of gene expression and
   metabolic pathways during low-temperature stress are discussed.}},
