@article{ ISI:A1995QM68800028,
Author = {KRISHNA, P and SACCO, M and CHERUTTI, JF and HILL, S},
Title = {{COLD-INDUCED ACCUMULATION OF HSP90 TRANSCRIPTS IN BRASSICA-NAPUS}},
