@article{ ISI:000404537300030,
Author = {Xu, Kai and Niu, Qingsheng and Zhao, Huiting and Du, Yali and Jiang,
   Yusuo},
Title = {{Transcriptomic analysis to uncover genes affecting cold resistance in
   the Chinese honey bee (Apis cerana cerana)}},
Journal = {{PLOS ONE}},
Year = {{2017}},
Volume = {{12}},
Number = {{6}},
Month = {{JUN 26}},
Abstract = {{The biological activity and geographical distribution of honey bees is
   strongly temperature-dependent, due to their ectothermic physiology. In
   China, the endemic Apis cerana cerana exhibits stronger cold hardiness
   than Western honey bees, making the former species important pollinators
   of winter-flowering plants. Although studies have examined behavioral
   and physiological mechanisms underlying cold resistance in bees, data
   are scarce regarding the exact molecular mechanisms. Here, we
   investigated gene expression in A. c. cerana under two temperature
   treatments, using transcriptomic analysis to identify differentially
   expressed genes (DEGs) and relevant biological processes, respectively.
   Across the temperature treatments, 501 DEGs were identified. A gene
   ontology analysis showed that DEGs were enriched in pathways related to
   sugar and amino acid biosynthesis and metabolism, as well as calcium ion
   channel activity. Additionally, heat shock proteins, zinc finger
   proteins, and serine/threonine-protein kinases were differentially
   expressed between the two treatments. The results of this study provide
   a general digital expression profile of thermoregulation genes
   responding to cold hardiness in A. c. cerana. Our data should prove
   valuable for future research on cold tolerance mechanisms in insects,
   and may be beneficial in breeding efforts to improve bee hardiness.}},
