@article{ ISI:000179486300033,
Author = {Sangwan, V and Dhindsa, RS},
Title = {{In vivo and in vitro activation of temperature-responsive plant map
   kinases}},
Journal = {{FEBS LETTERS}},
Year = {{2002}},
Volume = {{531}},
Number = {{3}},
Pages = {{561-564}},
Month = {{NOV 20}},
Abstract = {{Alfalfa cells possess two temperature-responsive Mitogen-Activated
   Protein Kinases (MAPKs), SAMK (Stress-Activated MAP Kinase) activated at
   4degreesC and HAMK (Heat shock-Activated MAP Kinase) activated at
   37degreesC. Both are inactive at 25degreesC. We show here that SAMK is
   activated when cells are transferred from 37degreesC to 25degreesC, and
   HAMK is activated when cells are transferred from 4degreesC to
   25degreesC. Moreover, we show that heat activation of HAMK also occurs
   in cell-free extracts. We conclude that (i) SAMK or HAMK activation does
   not require a particular temperature but a relative temperature shift,
   and (ii) that either HAMK itself or one or more of its upstream
   activators can sense temperature change directly. (C) 2002 Federation of
   European Biochemical Societies. Published by Elsevier Science B.V. All
   rights reserved.}},
